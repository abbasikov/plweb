{
  "name": "Lyceum",
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "description": "Enterprise Social Collaboration Product",
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "react",
    "redux",
    "webpack2"
  ],
  "author": "SyntaxFish",
  "bugs": {
    "url": ""
  },
  "homepage": "www.syntaxfish.com",
  "prepush": [
    "npm run lint-break-on-errors --silent"
  ],
  "scripts": {
    "test": "jest",
    "test-update": "jest -u",
    "start": "./node_modules/webpack-dashboard/bin/webpack-dashboard.js -t 'Lyceum' -- ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "build": "rm -rf ./build && NODE_ENV=\"production\" ./node_modules/webpack/bin/webpack.js",
    "lint-break-on-errors": "./node_modules/eslint/bin/eslint.js ./source/js -f table --ext .js --ext .jsx",
    "lint": "./node_modules/eslint/bin/eslint.js ./source/js -f table --ext .js --ext .jsx",
    "preview": "NODE_ENV=\"production\" ./node_modules/webpack-dashboard/bin/webpack-dashboard.js -t 'Preview Mode - React-Redux Boilerplate' -- ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "hook-add": "./node_modules/prepush/bin/cli.js install",
    "hook-remove": "./node_modules/prepush/bin/cli.js remove"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "./node_modules/babel-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "es6",
      "ts",
      "tsx",
      "js"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.7.2",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.6.1",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "0.14.5",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^3.10.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "jest": "^19.0.2",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "node-sass": "^3.13.0",
    "object.assign": "^4.0.4",
    "postcss-loader": "^1.1.1",
    "prepush": "^3.1.11",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "redux-mock-store": "^1.2.2",
    "sass-loader": "^4.0.2",
    "sinon": "^2.0.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.27",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^2.1.0-beta.10"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-polyfill": "^6.16.0",
    "es6-promise": "^3.1.2",
    "express": "^4.14.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "material-ui": "^0.17.4",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.0",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.3.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.0.1"
  }
}
